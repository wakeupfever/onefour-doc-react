{
  "name": "onefour-doc-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@better-scroll/core": "^2.2.1",
    "@better-scroll/observe-dom": "^2.2.1",
    "@better-scroll/pull-up": "^2.2.1",
    "@better-scroll/slide": "^2.2.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "axios": "^0.21.1",
    "concurrently": "^6.4.0",
    "good-storage": "^1.1.1",
    "node-sass": "^5.0.0",
    "pinyin": "^2.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.5",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "service": "node backend/prod.server.js",
    "start": "concurrently \"npm run dev\" \"npm run service\""
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/no-empty-function": "off",
      "vue/custom-event-name-casing": "off",
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-unused-vars": [
        "off"
      ],
      "no-unused-vars": "off",
      "space-before-function-paren": "off",
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        1,
        "single"
      ],
      "brace-style": "error",
      "indent": [
        "error",
        2
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": [
        "error",
        {
          "additionalHooks": "(useAsync|useAsyncCallback)"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {}
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react-app-rewire-alias": "^1.0.2",
    "react-app-rewired": "^2.1.8",
    "react-lazyload": "^3.2.0"
  }
}
